  margin-right: 10px;\r\n    }\r\n}\r\n.intro {\r\n    background-color: $gris-light;\r\n    padding: 30px 30px;\r\n    & .button {\r\n        color: $blanc;\r\n        font-weight: 500;\r\n        font-size: 12px;\r\n        // width: 180px;\r\n        // height: 40px;\r\n        padding: 15px 20px;\r\n        border-radius: 50px;\r\n        border: none;\r\n        background: linear-gradient(0deg, $violet 30%, $rose);\r\n        box-shadow: #a5a5a5 0 4px 8px;\r\n    }\r\n}","scss\\abstracts\\_variables.scss":"// COLORS\r\n$violet: #9356dc;\r\n$rose: #ff79da;\r\n$vert: #99e2d0;\r\n$blanc: #fff;\r\n$gris: #383838;\r\n$gris-light: #f0f0f0;","scss\\layout\\_footer.scss":"// FOOTER\r\n.footer {\r\n    background-color: $gris;\r\n    padding: 20px 20px;\r\n    color: $blanc;\r\n    p {\r\n        font-size: 15px;\r\n        margin: 0;\r\n        &.thin {\r\n            margin: 10px 0;\r\n        }\r\n    }\r\n    &-logo {\r\n        width: 100px;\r\n        height: auto;\r\n        margin: 10px 0;\r\n    }\r\n    &-icon {\r\n        margin-right: 15px;\r\n        & i {\r\n            width: 15px;\r\n            height: auto;\r\n        }\r\n    }\r\n    &-item {\r\n        &_flex {\r\n            margin: 10px 0;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n    }\r\n}","scss\\pages\\_accueil.scss":"// FONCTIONNEMENT\r\n.fonctionnement {\r\n    &-item {\r\n        display: flex;\r\n        align-items: center;\r\n        margin-bottom: 20px;\r\n    }\r\n    &-number {\r\n        width: 30px;\r\n        height: 30px;\r\n        border-radius: 15px;\r\n        margin-right: -15px;\r\n        z-index: 2;\r\n        color: $blanc;\r\n        background-color: $violet;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n    &-options {\r\n        // height: 50px;\r\n        width: calc(100% - 95px);\r\n        padding: 10px 40px;\r\n        border-radius: 15px;\r\n        background-color: $gris-light;\r\n        box-shadow: #c4c4c4 0 4px 10px;\r\n        display: flex;\r\n        align-items: center;\r\n        p {\r\n            font-size: 15px;\r\n            font-weight: bold;\r\n        }\r\n    }\r\n    &-icon {\r\n        color: lighten($gris, 30%);\r\n        margin-right: 30px;\r\n        font-size: 20px;\r\n    }\r\n}\r\n\r\n// RESTAURANTS\r\n.restaurants {\r\n    background-color: $gris-light;\r\n    a {\r\n        text-decoration: none !important;\r\n    }\r\n\r\n    &-visuel {\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin-bottom: 20px;\r\n        & img {\r\n            width: 100%;\r\n            height: 200px;\r\n            object-fit: cover;\r\n            object-position: center center;\r\n            border-radius: 20px 20px 0 0;\r\n        }\r\n        &_description {\r\n            box-shadow: 0 0 15px rgb(194, 194, 194);\r\n            background-color: white;\r\n            border-radius: 0 0 20px 20px;\r\n            padding: 10px;\r\n            position: relative;\r\n            font-size: 13px;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: space-between;\r\n            p {\r\n                margin-bottom: 5px;\r\n                color: black;\r\n            }\r\n        }\r\n        &_icon {\r\n            color: transparent;\r\n            font-size: 20px;\r\n            transform: scaleX(0.95);\r\n            &::before {\r\n                -webkit-text-stroke: 1px $gris;\r\n            }\r\n            &:hover {\r\n                background: -webkit-linear-gradient(-120deg, $violet 30%, $rose);\r\n                -webkit-background-clip: text;\r\n                -webkit-text-fill-color: transparent;\r\n                &::before {\r\n                    -webkit-text-stroke: 1px $blanc;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n","scss\\main.scss":"@import 'base/reset';\r\n@import 'base/fonts';\r\n\r\n@import 'abstracts/variables';\r\n@import 'abstracts/mixins';\r\n\r\n@import 'layout/nav';\r\n@import 'layout/header';\r\n@import 'layout/footer';\r\n\r\n@import 'pages/accueil';\r\n@import 'pages/restaurant';\r\n\r\n.blanc {\r\n    color: $blanc;\r\n}\r\n\r\nhtml {\r\n    font-family: \"Roboto\", sans-serif;\r\n}\r\n\r\nbody {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.title {\r\n    font-family: \"Shrikhand\", cursive;\r\n}\r\n\r\n.h1 {\r\n    @include fluid-text(25px, 30px, 1.1, 320px, 475px);\r\n    font-weight: 900;\r\n}\r\n.h4 {\r\n    @include fluid-text(15px, 18px, 1.1, 320px, 475px);\r\n    font-weight: 300;\r\n}\r\n\r\nsection {\r\n    padding: 30px 20px;\r\n}\r\n\r\n\r\n.violet {\r\n    color: $violet;\r\n    &_bg {\r\n        background-color: $violet!important;\r\n    }\r\n}\r\n\r\n.violet-light {\r\n    color: lighten($violet, 0.5);\r\n    &_bg {\r\n        background-color: lighten($violet, 35%);\r\n    }\r\n}\r\n\r\n.bold {\r\n    font-weight: bold!important;\r\n}\r\n\r\n.thin {\r\n    font-weight: 300!important;\r\n}","scss\\abstracts\\_mixins.scss":"// MIXINS\r\n\r\n@function strip-unit($number) {\r\n    @if type-of($number) == \"number\" and not unitless($number) {\r\n        @return $number / ($number * 0 + 1);\r\n    }\r\n\r\n    @return $number;\r\n}\r\n\r\n@mixin fluid-text($min-font-size, $max-font-size, $line-height, $break-start, $break-end) {\r\n    /////\r\n    // Calculations font size\r\n    /////\r\n    $fsm: (strip-unit($max-font-size) - strip-unit($min-font-size)) /\r\n        (strip-unit($break-end) - strip-unit($break-start));\r\n    $fsb: #{$min-font-size - $fsm * $break-start};\r\n    $fsmx: #{$fsm * 100vw};\r\n    /////\r\n    // Output\r\n    /////\r\n    font-size: #{$min-font-size};\r\n    line-height: #{$line-height};\r\n\r\n    @media (min-width: #{$break-start}) {\r\n        font-size: calc(#{$fsmx} + #{$fsb});\r\n    }\r\n\r\n    @media (min-width: $break-end) {\r\n        font-size: #{$max-font-size};\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"19fe96fbb1a2ae3992cfd73a7165301d","cacheData":{"env":{}}}